{
  "extends": "@astriel-division/ts-config",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist/",
    "paths": {
      "#apex/api": ["../api/src/index"],
      "#apex/api/*": ["../api/src/*"]
    },
		"importHelpers": false,
    "noEmitHelpers": false,
		"importsNotUsedAsValues": "preserve",
    "resolveJsonModule": true
  },
  "include": [
    "../api/**/*.ts",
    "../api/src/config.json",
    "./src/*.ts"
  ], 
  "exclude": []
}
